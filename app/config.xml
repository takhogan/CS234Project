<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.donotdisturb" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Do Not Disturb!</name>
    <description>
        Context-based notification management system.
    </description>
    <author email="" href="">
        1984 But This Time There's Netflix
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/img/icon.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="www/img/icon.png" />
    </platform>
    <platform name="browser">
        <icon src="www/img/icon.png" />
    </platform>
</widget>
